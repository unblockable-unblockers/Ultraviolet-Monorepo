{
  "name": "uv-app",
  "version": "1.0.0",
  "description": "Ultraviolet instance",
  "type": "module",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "build": "pkg -d --public-packages \"*\" ."
  },
  "keywords": [
    "proxy"
  ],
  "author": "",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@mercuryworkshop/bare-mux": "^1.0.4",
    "@mercuryworkshop/epoxy-transport": "^1.0.2",
    "@titaniumnetwork-dev/ultraviolet": "workspace:*",
    "express": "^4.18.2",
    "ultraviolet-static": "workspace:*",
    "wisp-server-node": "^1.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@yao-pkg/pkg": "^5.11.5",
    "eslint": "^8.36.0",
    "prettier": "^2.8.4"
  },
  "bin": "./src/index.js",
  "pkg": {
    "scripts": [
      "./node_modules/@mercuryworkshop/bare-mux/lib/index.cjs",
      "./node_modules/@mercuryworkshop/epoxy-transport/lib/index.cjs",
      "!./node_modules/**/dist/*",
      "!./node_modules/**/public/*"
    ],
    "assets": [
      "./node_modules/**/dist/*",
      "./node_modules/**/public/*"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-win-x64"
    ],
    "outputPath": "dist"
  }
}
